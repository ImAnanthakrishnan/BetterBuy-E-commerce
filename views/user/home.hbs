<section class="" style="background-color: #fff5ee !important;">
 <div id="carouselExampleIndicators" class="carousel slide" data-bs-ride="carousel">
  <div class="carousel-indicators">
    <button type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide-to="0" class="active" aria-current="true" aria-label="Slide 1"></button>
    <button type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide-to="1" aria-label="Slide 2"></button>
    <button type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide-to="2" aria-label="Slide 3"></button>
  </div>
  <div class="carousel-inner">
    <div class="carousel-item active">
       <div class="wrapper">
        <div class="image"> 
<img src="images/2.jpg" class="d-block w-100" alt="...">
          <div class="content">
            <h1>Hello {{data}},Welcome</h1>
            <a href="/grocery"><button class="btn ">Shop now</button></a>
          </div>
        </div>
       </div>
      
     <!--<div class="carousel-caption d-none d-md-block">
         <h5>First slide label</h5>
        <p>Some representative placeholder content for the first slide.</p>
       </div>-->
    </div>
    <div class="carousel-item">
      <img src="images/fruits.jpg" class="d-block w-100" alt="...">
      <div class="carousel-caption d-none d-md-block">
         <h5 class=" fs-4">Fresh fruits</h5>
        <p class=" fs-3">We are providing organic fruits check it out.</p>
       </div>
    </div>
    <div class="carousel-item">
      <img src="images/1.jpg" class="d-block w-100" alt="...">
      <div class="carousel-caption d-none d-md-block">
         <h5>Add spices to make food delicious</h5>
        <p>We have the different varities of spices available</p>
       </div>
    </div>
  </div>
  <button class="carousel-control-prev" type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide="prev">
    <span class="carousel-control-prev-icon" aria-hidden="true"></span>
    <span class="visually-hidden">Previous</span>
  </button>
  <button class="carousel-control-next" type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide="next">
    <span class="carousel-control-next-icon" aria-hidden="true"></span>
    <span class="visually-hidden">Next</span>
  </button>
</div>


<div class="d-flex justify-content-around p-3" style="height: 100px; background-color:lightgreen">
    <div class="d-flex">
        <div class="bi bi-box me-3 icon-bi" style="font-size: 50px;"></div>
        <div class="mt-2">
            <h6 class="text">Free Local Delivery</h6>
            <p class="text1">On all orders over 100</p>
        </div>
    </div>
     <div class="d-flex">
        <div class="fa-solid fa-leaf  me-3 mt-2 icon-bi" style="font-size: 50px;"></div>
        <div class="mt-2">
            <h6 class="text">Freshness Guarantee</h6>
            <p class="text1">From farm to fork</p>
        </div>
    </div>
     <div class="d-flex">
        <div class="fa-regular fa-circle-check  me-3 mt-2 icon-bi" style="font-size: 50px;"></div>
        <div class="mt-2">
            <h6 class="text">Quality assured</h6>
            <p class="text1">Fill your basket faster</p>
        </div>
    </div>
</div>

